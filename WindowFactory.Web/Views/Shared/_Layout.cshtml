<!DOCTYPE html>
<html lang="ru">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <title>Оконный завод "Престиж"</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <link href='//fonts.googleapis.com/css?family=Roboto:400,100,100italic,300,300italic,400italic,500,500italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>
    <link href="~/css/bootstrap.min.css" rel='stylesheet' type='text/css' />
    <!-- Custom Theme files -->
    <link href="~/css/style.css" rel='stylesheet' type='text/css' />
    <link href="~/css/font-awesome.css" rel="stylesheet">
    <script src="~/js/jquery.min.js"></script>
    <!-- Mainly scripts -->
    <script src="~/js/jquery.metisMenu.js"></script>
    <script src="~/js/jquery.slimscroll.min.js"></script>
    <!-- Custom and plugin javascript -->
    <link href="~/css/custom.css" rel="stylesheet">
    <script src="~/js/custom.js"></script>
    <script src="~/js/screenfull.js"></script>
    <script src="~/js/Chart.js"></script>
    <script>
        $(function () {
            $('#supported').text('Supported/allowed: ' + !!screenfull.enabled);

            if (!screenfull.enabled) {
                return false;
            }

            $('#toggle').click(function () {
                screenfull.toggle($('#container')[0]);
            });
        });
    </script>

    <!----->
    <!--pie-chart--->
    <script src="/js/pie-chart.js"></script>

    <!--skycons-icons-->
    <script src="~/js/skycons.js"></script>
    <!--//skycons-icons-->
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body style="display: none" data-bind="visible: true">
    <div id="wrapper">
        <nav class="navbar-default navbar-static-top" role="navigation">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Закрепить панель</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <h1> <a class="navbar-brand" href="index.html">Престиж</a></h1>
            </div>
            <div class="border-bottom">
                <div class="full-left">
                    <section class="full-top">
                        <button id="toggle"><i class="fa fa-arrows-alt"></i></button>
                    </section>
                    <form class=" navbar-left-right">
                        <input type="text" value="Поиск..." onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Поиск...';}">
                        <input type="submit" value="" class="fa fa-search">
                    </form>
                    <div class="clearfix"> </div>
                </div>

                @Html.Partial("_Navbar")
                @Html.Partial("_Sidebar")
            </div>
        </nav>

        <div id="page-wrapper" class="gray-bg dashbard-1">
            <div class="content-main">
                @*<!--banner-->
                <div class="banner">
                    <h2>
                        <a href="index.html">Home</a>
                        <i class="fa fa-angle-right"></i>
                        <span>Dashboard</span>
                    </h2>
                </div>
                <!--//banner-->
                <!--content-->
                <div class="content-top">
                    <div class="clearfix"></div>
                </div>
                <!---->
                <div class="content-mid">
                    <div class="clearfix"> </div>
                </div>
                <!----->
                <div class="content-bottom">
                    <div class="clearfix"> </div>
                </div>
                <!--//content-->*@
                <!---->

                @RenderBody()
                @RenderSection("SPAViews", required: false)

                @Html.Partial("_Footer")
            </div>
            <div class="clearfix"> </div>
        </div>
    </div>

    <!--scrolling js-->
    <script src="~/js/jquery.nicescroll.js"></script>
    <script src="~/js/scripts.js"></script>
    <!--//scrolling js-->
    <script src="~/js/bootstrap.min.js"></script>

    @Scripts.Render("~/bundles/highcharts")
    @RenderSection("Scripts", required: false)
</body>
</html>
